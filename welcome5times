public class WelcomeTimes5 { //Public class block defined WelcomeTimes5

	public static void main(String[] args) { //Main method block defined
		System.out.println("Welcome to Java"); //Welcome to Java Display Statement
		System.out.println("Welcome to Java"); //Welcome to Java Display Statement Again
		System.out.println("Welcome to Java"); //Welcome to Java Display Statement Again
		System.out.println("Welcome to Java"); //Welcome to Java Display Statement Again
		System.out.println("Welcome to Java"); //Welcome to Java Display Statement Again

	} //End of main method block

} //End of WelcomeTimes5 class
